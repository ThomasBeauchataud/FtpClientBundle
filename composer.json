{
    "name": "tbcd/ftp-client-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for tbcd/ftp-client allowing ftp client creation in the container",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "keywords": [
        "ftp",
        "sftp",
        "scp",
        "ftps"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.1",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "ext-ssh2": "*",
        "tbcd/ftp-client": "^1.1",
        "symfony/framework-bundle": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\FtpClientBundle\\": "src/"
        }
    }
}
